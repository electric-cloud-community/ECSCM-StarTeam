<?xml version="1.0" encoding="UTF-8"?>
<plugin>
    <key>ECSCM-StarTeam</key>
    <version>@PLUGIN_VERSION@</version>
    <label>ECSCM-StarTeam</label>
    <description>SCM Plugin for StarTeam</description>
    <author>Electric Cloud</author>
    <authorUrl>http://www.electric-cloud.com/support</authorUrl>
    <category>Source Code Management</category>
    <commander-version min="3.6"/>
    <depends min="1.0">ECSCM</depends>
		
	<customTypes>
	    <customType name="reports">
          <displayName>Display StarTeam Changelog Report</displayName>
          <description></description>
          <page pageName="STReports" definition="pages/reports.xml"/>
        </customType>
   </customTypes>
   <components>
      <component name="Reports">
      <javascript>war/ecplugins.ECSCM.Reports/ecplugins.ECSCM.Reports.nocache.js</javascript>
     </component>
   </components>
   
   <help>ECSCM-StarTeam_help.xml</help>        
</plugin>
